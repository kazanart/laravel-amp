{
    "name": "shaneeardley80/laravel-amp",
    "description": "Package that helps you set up AMP (Accelerated Mobile Pages) using Laravel",
    "keywords": [
        "shaneeardley80",
        "laravel-amp"
    ],
    "homepage": "https://github.com/shaneeardley80",
    "license": "MIT",
    "authors": [
        {
            "name": ":Shane Eardley",
            "email": "shane.eardley.80@gmail.com",
            "homepage": "https://github.com/shaneeardley80",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5",
        "illuminate/contracts": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Just\\Amp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Just\\Amp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Just\\Amp\\Laravel\\AmpServiceProvider"
            ],
            "aliases": {
                "AmpRouter": "Just\\Amp\\Laravel\\AmpRouteFacade"
            }
        }
    }
}
